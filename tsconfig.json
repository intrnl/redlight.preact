{
	"compilerOptions": {
		"noEmit": true,
		"allowJs": true,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true,
		"alwaysStrict": true,
		"jsx": "react",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment",
		"paths": {
			"~/*": ["./*"],
    }
	}
}
